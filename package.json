{
  "name": "valentinalistings",
  "version": "0.0.0",
  "private": true,
  "main": "start.js",
  "scripts": {
    "build": "webpack & webpack --config webpack.config.production.js",
    "dev": "node webpack-dev-server.js",
    "lint": "eslint ./*.js ./**/*.js ./*.css ./**/*.css",
    "start": "npm run build && node start.js",
    "test": "NODE_ENV=test BABEL_ENV=test XUNIT_FILE=artifacts/test/xunit.xml nyc --report-dir ${COVERAGE_DIR:-artifacts/coverage} --reporter lcov mocha --no-timeouts --exit --max-old-space-size=4096 --require babel-register,./tests/lib/enzyme.js tests/unit --recursive",
    "testDir": "NODE_ENV=test mocha --no-timeouts --exit --require babel-register,./tests/lib/enzyme.js --recursive",
    "commit-test": "NODE_ENV=test BABEL_ENV=test mocha --no-timeouts --exit --max-old-space-size=4096 --require babel-register,./tests/lib/enzyme.js tests/unit --recursive"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.6.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "body-parser": "^1.6.4",
    "compression": "^1.5.1",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "debug": "^2.0.0",
    "express": "^4.3.2",
    "express-state": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fluxible": "^1.0.0",
    "fluxible-action-utils": "^0.2.4",
    "fluxible-addons-react": "^0.2.0",
    "fluxible-plugin-fetchr": "^0.3.0",
    "fluxible-router": "^0.4.0",
    "querystring": "^0.2.0",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.1.6",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-stage-3": "^6.24.1",
    "bundle-loader": "^0.5.0",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-15": "^1.4.1",
    "eslint": "^4.18.2",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.1.0",
    "json-loader": "^0.5.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^1.2.8",
    "react-test-renderer": "^15.6.2",
    "shelljs": "^0.6.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "webpack": "^1.12.4",
    "webpack-atomizer-loader": "^1.0.2",
    "webpack-dev-server": "^1.6.5"
  },
  "pre-commit": [
    "lint",
    "commit-test"
  ]
